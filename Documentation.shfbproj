    <DocumentationSources>
      <DocumentationSource sourceFile="bin\DW.CodedUI.dll" />
      <DocumentationSource sourceFile="bin\DW.CodedUI.xml" />
    </DocumentationSources>
    <VisibleItems>Protected, EditorBrowsableNever, NonBrowsable</VisibleItems>
